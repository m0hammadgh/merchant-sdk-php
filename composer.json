{
    "name": "yourusername/paypal-merchant-sdk-php",
    "description": "Fork of PayPal Merchant SDK for PHP",
    "keywords": ["paypal", "php", "sdk"],
    "homepage": "https://developer.paypal.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "PayPal",
            "homepage": "https://github.com/paypal/merchant-sdk-php/contributors"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/m0hammadgh/merchant-sdk-php.git",
            "type": "vcs"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "brianreavis/sdk-core-php":"*"
    },
    "replace": {
        "paypal/merchant-sdk-php": "*"
    },
    "autoload": {
        "psr-0": {
            "PayPal\\Service": "lib/",
            "PayPal\\CoreComponentTypes": "lib/",
            "PayPal\\EBLBaseComponents": "lib/",
            "PayPal\\EnhancedDataTypes": "lib/",
            "PayPal\\PayPalAPI": "lib/"
        }
    }
}
